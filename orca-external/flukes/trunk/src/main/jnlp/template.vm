<?xml version="1.0" encoding="utf-8"?>
<jnlp
		spec="1.0+"
		codebase="$project.Url/$project.Version"
		href="$outputFile">
	<information>
		<title>$project.Name</title>
		<vendor>$project.Organization.Name</vendor>
		<homepage href="$project.Organization.Url"/>
		<description>$project.Description</description>
		<icon kind="splash" href="$project.Url/$project.Version/html/icon.png"/>
		<icon href="$project.Url/$project.Version/html/icon.png"/>
	</information>
	<security>
		<all-permissions/>
	</security>
	<resources>
		<j2se version="1.6+" max-heap-size="1024m" />
		$dependencies
		<extension name="BouncyCastle Provider library" href="bcprov.jnlp"/>
		<extension name="BouncyCastle Provider Extensions library" href="bcprov-ext.jnlp"/>
		<extension name="BouncyCastle Public Key Infrastructure library" href="bcpkix.jnlp"/>
	</resources>
	<application-desc main-class="$mainClass"/>
</jnlp>
